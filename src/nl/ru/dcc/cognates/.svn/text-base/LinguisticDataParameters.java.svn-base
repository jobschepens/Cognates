package nl.ru.dcc.cognates;

public class LinguisticDataParameters {
	private int MAXTRANSLATIONPAIRS;
	private double oTHRESHOLD, pTHRESHOLD;
	private int MINLENGTH;
	private int MAXLENGTH;
	private double MINFREQUENCY;
	private String FREQTYPE;
	private boolean COUNTMATCHES;

	public LinguisticDataParameters(int mAXTRANSLATIONPAIRS, double tHRESHOLD,
			double threshold2, int mINLENGTH, int mAXLENGTH, double mINFREQUENCY, String fREQTYPE, boolean cOUNTMATCHES) {
		MAXTRANSLATIONPAIRS = mAXTRANSLATIONPAIRS;
		oTHRESHOLD = tHRESHOLD;
		pTHRESHOLD = threshold2;
		MINLENGTH = mINLENGTH;
		MAXLENGTH = mAXLENGTH;
		MINFREQUENCY = mINFREQUENCY;
		FREQTYPE = fREQTYPE;
		setCOUNTMATCHES(cOUNTMATCHES);
	}

	public int getMaxPairs() {
		return MAXTRANSLATIONPAIRS;
	}

	public void setMAXCOGNATES(int mAXCOGNATES) {
		MAXTRANSLATIONPAIRS = mAXCOGNATES;
	}

	public double getOTHRESHOLD() {
		return oTHRESHOLD;
	}

	public void setOTHRESHOLD(double OtHRESHOLD) {
		oTHRESHOLD = OtHRESHOLD;
	}

	/**
	 * @param pTHRESHOLD the pTHRESHOLD to set
	 */
	public void setPTHRESHOLD(double pTHRESHOLD) {
		this.pTHRESHOLD = pTHRESHOLD;
	}

	/**
	 * @return the pTHRESHOLD
	 */
	public double getPTHRESHOLD() {
		return pTHRESHOLD;
	}

	public int getMINLENGTH() {
		return MINLENGTH;
	}

	public void setMINLENGTH(int mINLENGTH) {
		MINLENGTH = mINLENGTH;
	}

	public int getMAXLENGTH() {
		return MAXLENGTH;
	}

	public void setMAXLENGTH(int mAXLENGTH) {
		MAXLENGTH = mAXLENGTH;
	}

	public double getMINFREQUENCY() {
		return MINFREQUENCY;
	}

	public void setMINFREQUENCY(double mINFREQUENCY) {
		MINFREQUENCY = mINFREQUENCY;
	}

	public String getFREQTYPE() {
		return FREQTYPE;
	}

	public void setFREQTYPE(String fREQTYPE) {
		FREQTYPE = fREQTYPE;
	}

	/**
	 * @param cOUNTMATCHES the cOUNTMATCHES to set
	 */
	public void setCOUNTMATCHES(boolean cOUNTMATCHES) {
		COUNTMATCHES = cOUNTMATCHES;
	}

	/**
	 * @return the cOUNTMATCHES
	 */
	public boolean isCOUNTMATCHES() {
		return COUNTMATCHES;
	}
}